<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="cygnus"
           fetch="https://github.com/cygnus-next"
           revision="ten" />

  <remote name="lineage"
          fetch="https://github.com/LineageOS"
          revision="lineage-17.1" />


  <!-- Build -->
  <project path="build/make" name="build" groups="pdk" remote="cygnus">
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="build_soong" groups="pdk,tradefed" remote="cygnus" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- Vendor -->
  <project path="vendor/cygnus" name="vendor_cygnus" remote="cygnus" />
  <project path="vendor/lineage" name="vendor_lineage" remote="cygnus" />

  <!-- Frameworks -->
  <project path="frameworks/base" name="frameworks_base" remote="cygnus" />
  
  <!-- Settings -->
  <project path="packages/apps/Settings" name="packages_apps_Settings" remote="cygnus" />
  
  <!-- System Core -->
  <project path="system/core" name="system_core" remote="cygnus" />

  <!-- Telcomm --> 
  <project path="packages/services/Telecomm" name="packages_services_Telecomm" remote="cygnus" />>
  <!-- Apps -->
  <project path="packages/apps/Spaceship" name="packages_apps_Spaceship" remote="cygnus" />
  <project path="packages/apps/LineageParts" name="packages_apps_LineageParts" remote="cygnus" revision="ten-rebase" />
  <project path="packages/apps/PermissionController" name="packages_apps_PackageInstaller" remote="cygnus" />
  <project path="packages/apps/Profiles" name="android_packages_apps_Profiles" remote="lineage" />
  <project path="packages/apps/Backgrounds" name="android_packages_apps_Backgrounds" remote="lineage" revision="lineage-16.0" />
  <project path="packages/apps/SetupWizard" name="packages_apps_SetupWizard" remote="cygnus" />
  <!-- Extra packages not included in the main manifest -->
  <project path="packages/resources/devicesettings" name="android_packages_resources_devicesettings" remote="lineage" />
  <project path="packages/apps/FlipFlap" name="android_packages_apps_FlipFlap" remote="lineage"/>
  <!-- Remove LineageOS Apps-->

  <remove-project groups="pdk-fs" name="LineageOS/android_packages_screensavers_Basic" path="packages/screensavers/Basic" />

  <remove-project groups="pdk-fs" name="LineageOS/android_packages_screensavers_PhotoTable" path="packages/screensavers/PhotoTable" />

  <remove-project groups="pdk-fs" name="LineageOS/android_packages_wallpapers_LivePicker" path="packages/wallpapers/LivePicker" />

  <remove-project name="LineageOS/android_packages_apps_AudioFX" path="packages/apps/AudioFX" />

  <remove-project name="LineageOS/android_packages_apps_Calendar" path="packages/apps/Calendar" />

  
  <remove-project name="LineageOS/android_packages_apps_CellBroadcastReceiver" path="packages/apps/CellBroadcastReceiver" />

  <remove-project name="LineageOS/android_packages_apps_DeskClock" path="packages/apps/DeskClock" />

  

  <remove-project name="LineageOS/android_packages_apps_Exchange" path="packages/apps/Exchange" />

  <remove-project name="LineageOS/android_packages_apps_Email" path="packages/apps/Email" />

  <remove-project name="LineageOS/android_packages_apps_EmergencyInfo" path="packages/apps/EmergencyInfo" />

  <remove-project name="LineageOS/android_packages_apps_FMRadio" path="packages/apps/FMRadio" />

  <remove-project name="LineageOS/android_vendor_qcom_opensource_fm-commonsys" path="vendor/qcom/opensource_fm-commonsys" />

  <remove-project name="LineageOS/android_packages_apps_Jelly" path="packages/apps/Jelly" />

  <remove-project name="LineageOS/android_packages_apps_Nfc" path="packages/apps/Nfc" />

  <remove-project name="LineageOS/android_packages_apps_TV" path="packages/apps/TV" />

  <remove-project name="LineageOS/android_packages_apps_TvSettings" path="packages/apps/TvSettings" />

  <remove-project name="LineageOS/android_packages_apps_ThemePicker" path="packages/apps/ThemePicker" />

  <remove-project name="LineageOS/android_packages_apps_WallpaperPicker2" path="packages/apps/WallpaperPicker2" />

  <remove-project name="LineageOS/android_packages_apps_Updater" path="packages/apps/Updater" />

  <remove-project name="LineageOS/android_packages_apps_ManagedProvisioning" path="packages/apps/ManagedProvisioning" />

  <remove-project name="LineageOS/android_packages_apps_Snap" path="packages/apps/Snap" />

  <remove-project name="LineageOS/android_packages_apps_SafetyRegulatoryInfo" path="packages/apps/SafetyRegulatoryInfo" />

  <remove-project name="LineageOS/android_packages_providers_BookmarkProvider" path="packages/providers/BookmarkProvider" />

  <remove-project name="LineageOS/android_packages_providers_PartnerBookmarksProvider" path="packages/providers/PartnerBookmarksProvider" />

  <remove-project name="LineageOS/android_packages_providers_TvProvider" path="packages/providers/TvProvider" />

  
  <remove-project groups="pdk-fs" name="platform/packages/apps/OneTimeInitializer" path="packages/apps/OneTimeInitializer" />

  <remove-project groups="pdk-fs" name="platform/packages/apps/Provision" path="packages/apps/Provision" />

  <remove-project name="platform/packages/apps/UniversalMediaPlayer" path="packages/apps/UniversalMediaPlayer" />
  
  <remote name="opengapps" fetch="https://github.com/opengapps/"  />
<remote name="gitlab-gapps" fetch="https://gitlab.opengapps.org/opengapps/"  />

<project path="vendor/opengapps/build" name="aosp_build" revision="master" remote="opengapps" />

<project path="vendor/opengapps/sources/all" name="all" clone-depth="1" revision="master" remote="gitlab-gapps" />

<!-- arm64 depends on arm -->
<project path="vendor/opengapps/sources/arm" name="arm" clone-depth="1" revision="master" remote="gitlab-gapps" />
<project path="vendor/opengapps/sources/arm64" name="arm64" clone-depth="1" revision="master" remote="gitlab-gapps" />

<project path="vendor/opengapps/sources/x86" name="x86" clone-depth="1" revision="master" remote="gitlab-gapps" />
<project path="vendor/opengapps/sources/x86_64" name="x86_64" clone-depth="1" revision="master" remote="gitlab-gapps" />

</manifest>
